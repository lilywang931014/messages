<div class="container text-center mb-4">
  <div class="row">
    <div class="col-sm-4">
      <h1>Register</h1>
      <form class="form-signin" action="/register" method="POST">

      <div class="form-label-group">
        <label class="sr-only" for="email">Email</label>
        <input id="inputEmail" class="form-control" type="email" name="username" placeholder="Email Address" required autofocus>
      </div >

      <div class="form-label-group">
      <label class="sr-only" for="password">Password</label>
      <input id="inputPassword" class="form-control" type="password" name="password" placeholder="Password" required>
      </div>

      <div class="col-sm-4">
        <div class="card social-block">
          <div class="card-body">
            <a class="btn btn-block btn-social btn-google" href="/auth/google" role="button">
              <i class="fab fa-google"></i>
              Sign Up with Google
            </a>
          </div>
        </div>
      </form>
    </div>

  </div>

</div>

</div>

